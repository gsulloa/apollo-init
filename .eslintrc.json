{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "func-names": "off",
    "no-param-reassign": "off",
    "camelcase": "warn",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "prettier/prettier": "error"
  }
}
